<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<display-name>Uravel</display-name>
	<welcome-file-list>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>UC</servlet-name>
		<servlet-class>com.controller.UravelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UC</servlet-name>
		<url-pattern>/UC</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UC</servlet-name>
		<url-pattern>/main/UC</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UC</servlet-name>
		<url-pattern>/Controller</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UC</servlet-name>
		<url-pattern>/main/Controller</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UC</servlet-name>
		<url-pattern>/common/Controller</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UC</servlet-name>
		<url-pattern>/admin/Controller</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UC</servlet-name>
		<url-pattern>/history_Controller</url-pattern>
	</servlet-mapping>
</web-app>